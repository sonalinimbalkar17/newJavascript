//class => Es6 javascript  => esma script
//class => blue print => format create => multiple object

//1.setting object property outside of the class

class Student{

    firstName
    lastName
    rollNo
    age
    city
}

let aarti=new Student()
console.log(aarti)
//curd operation
aarti.firstName="aarti"
aarti.lastName="nimbalkar"
aarti.rollNo=5
aarti.age=18
aarti.city="nashik"
console.log(aarti)

let disha=new Student()
console.log(disha)
//curd operation
disha.firstName="disha"
disha.lastName="nimbalkar"
disha.rollNo=15
disha.age=16
disha.city="nagpur"
console.log(disha)

//2. class =>  by using constructor
class Person{
    constructor(fn,ln,rn,ag,city){
        this.firstName=fn
        this.lastName=ln
        this.rollNo=rn
        this.age=ag
        this.city=city
    }
}

let kalyani=new Person("kalyani","hadole",20,23,"mumbai")
console.log(kalyani)

//3. by using function
class Person1{
    setfirstName(fn){
        this.firstName=fn
    }

    setlastname(ln){
        this.lastName=ln
    }

    setrollNo(rn){
        this.rollNo=rn
    }

    setage(ag){
        this.age=ag
    }

    setcity(city){
        this.city=city
    }
}

let sai=new Person1()
console.log(sai)//empty object
sai.setfirstName="sai"
sai.setlastname="nimbalkar"
sai.setrollNo=10
sai.setage=8
sai.setcity="nagpur"
console.log(sai)

